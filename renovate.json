{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "workspaces/prod-home/nomad.tf"
      ],
      "matchStrings": [
        ".+_version\\s*=\\s*\"(?<currentValue>.+)\"\\s*#\\s*image:\\s*(?<depName>.+)\\n"
      ],
      "datasourceTemplate": "docker"
    }
  ]
}
